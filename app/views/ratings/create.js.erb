<% if @error.present? %>
$("#error").html("<%= escape_javascript(@error) %>");
$("#error").addClass("error");
<% else %>
$("#star-ratings-block_<%= @rateable.id %>").html("<%= escape_javascript(render :partial => "rating", :locals => { :asset => @rateable }) %>");
$("#block_<%= @rateable.id %>").html("<%= escape_javascript(render("thanks")) %>");
<% end %>